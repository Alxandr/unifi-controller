{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": "^\\.env$",
      "matchStrings": [
        "export UNIFI_CONTROLLER_VERSION=(?<currentValue>.*?)\n"
      ],
      "depNameTemplate": "ghcr.io/linuxserver/unifi-controller",
      "datasourceTemplate": "docker"
    }
  ]
}
